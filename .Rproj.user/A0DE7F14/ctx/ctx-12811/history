u_outlier <- function(data, coef = 1.5) {
u.limit = quantile(data, .75)[[1]] + IQR(data)*coef
return(u.limit)
}
u_outlier(c(1,2,3,4))
u_outlier(c(1,2,3,4), coef=0.5)
