outlier <- function(data, coef = 1.5) {
u.limit = quantile(data, .75)[[1]] + IQR(data)*coef
l.limit = quantile(data, .25)[[1]] - IQR(data)*coef
return(cbind(u.limit, l.limit))
}
aggregate(b ~ a, fra, outlier)
outlier <- function(data, coef = 1.5) {
u.limit = quantile(data, .75)[[1]] + IQR(data)*coef
l.limit = quantile(data, .25)[[1]] - IQR(data)*coef
return(cbind(upper.limit = u.limit, lower.limit = l.limit))
}
aggregate(b ~ a, fra, outlier)
View(outlier)
